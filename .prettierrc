{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "importOrder": [
    "next",
    "^(?!next)\\w+$",
    "react",
    "^(?!react)\\w+$",
    "^(?!.*/).*$",
    "^@features/(.*)$",
    "^@components/(.*)$",
    "^@styles/(.*)$",
    "^@t/(.*)$",
    "^@stories/(.*)$",
    "^(..)",
    "^(.)"
  ],
  "jsxSingleQuote": false,
  "printWidth": 80,
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false
}
